      RowBox[{"P11", "[", 
       RowBox[{"s", ",", " ", "pik"}], "]"}], "*", 
      RowBox[{"P11", "[", 
       RowBox[{"r", ",", " ", "pik"}], "]"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", " ", "-", " ", "Ptk"}], ")"}], "*", 
      RowBox[{"P01", "[", 
       RowBox[{"s", ",", " ", "pik"}], "]"}], "*", 
      RowBox[{"P01", "[", 
       RowBox[{"r", ",", " ", "pik"}], "]"}]}]}]}], ";"}],
