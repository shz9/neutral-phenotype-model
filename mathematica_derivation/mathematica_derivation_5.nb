Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{"r", "+", "s", "+", "t"}], ")"}]}], " ", "u"}]], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     SubscriptBox["p0", "k"], " ", 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "2"}], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"r", " ", "u"}]], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"s", " ", "u"}]]}], ")"}], " ", 
        SubscriptBox["\[Pi]", "k"]}]}], ")"}]}], "-", 
    RowBox[{
     SubscriptBox["\[Pi]", "k"], " ", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"t", " ", "u"}]], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "2"}], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"r", " ", "u"}]], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"s", " ", "u"}]], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"t", " ", "u"}]], "-", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"r", "+", "s", "+", "t"}], ")"}], " ", "u"}]]}], ")"}], 
        " ", 
        SubscriptBox["\[Pi]", "k"]}]}], ")"}]}]}], ")"}]}]], "Output",
 CellChangeTimes->{3.821322182902008*^9},
